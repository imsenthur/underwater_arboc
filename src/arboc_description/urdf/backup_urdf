<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="arboc">

  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="mass" value="0.140603015937" />
  <xacro:property name="radius" value="0.0335" /> 
  <xacro:property name="height" value="0.040" />
  <xacro:property name="_mass" value="0.228479900898" />
  <xacro:property name="_height" value="0.065" />

  <!-- ros_control plugin -->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/arboc</robotNamespace>
      <!-- <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType> -->
    </plugin>
  </gazebo>

  <link name="dummy_root"/>

  <joint name="dummy_root_to_link" type="fixed">
    <parent link="dummy_root"/>
    <child link="link_01"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="link_01">
    <collision>
      <origin xyz="0 0 0" rpy="0 ${-PI/2} 0" />
      <geometry>
        <cylinder radius="${radius}" length="${height}" />
      </geometry>
    </collision> 

    <surface>
      <friction>
        <ode>
          <mu>0.998000</mu>
          <mu2>0.998000</mu2>
          <fdir1>0.000000 0.000000 0.000000</fdir1>
          <slip1>0.000000</slip1>
          <slip2>0.000000</slip2>
        </ode>
      </friction>
      <bounce>
        <restitution_coefficient>0.000000</restitution_coefficient>
        <threshold>100000.000000</threshold>
      </bounce>
      <contact>
        <ode>
          <soft_cfm>0.000000</soft_cfm>
          <soft_erp>0.200000</soft_erp>
          <kp>100000.000000</kp>
          <kd>1.000000</kd>
          <max_vel>100.000000</max_vel>
          <min_depth>0.001000</min_depth>
        </ode>
      </contact>
    </surface>

    <visual>
       <origin xyz="0 0 0" rpy="0 ${-PI/2} 0" />
      <geometry>
        <cylinder radius="${radius}" length="${height}" />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="${mass}"/>
      <inertia 
          ixx="${mass/2.0*radius**2}" 
          iyy="${mass/12.0*(3*radius**2+height**2)}" 
          izz="${mass/12.0*(3*radius**2+height**2)}" 
          ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="joint_01" type="revolute">
    <parent link="link_01"/>
    <child link="link_02"/>
    <origin xyz="${height/2} 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping=".1" friction="0.2"/>
    <limit effort="20" lower="-1.57" upper="1.57" velocity="2.208932"/>
  </joint>

  <transmission name="joint_01_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_01">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="joint_01_motor">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>300</mechanicalReduction>
    </actuator>
  </transmission>

